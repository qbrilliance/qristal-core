cmake_minimum_required(VERSION @MIN_CMAKE_VERSION@ FATAL_ERROR)
project(poorly_behaved_dependency_dry_run LANGUAGES CXX)
find_package(@arg_FIND_PACKAGE_NAME@ QUIET HINTS @dir@)
if(@arg_FIND_PACKAGE_NAME@_FOUND)
  string(FIND "${@arg_FIND_PACKAGE_NAME@_VERSION}" "@VERSION@-@arg_GIT_TAG@" MATCH1)
  string(FIND "@VERSION@-@arg_GIT_TAG@" "${@arg_FIND_PACKAGE_NAME@_VERSION}" MATCH2)
  if(NOT MATCH1 EQUAL -1 OR NOT MATCH2 EQUAL -1)
    message("poorly_behaved_dependency_dry_run: Found")
  else()
    if(@arg_FIND_PACKAGE_NAME@_VERSION)
      message("poorly_behaved_dependency_dry_run: v${@arg_FIND_PACKAGE_NAME@_VERSION}, found at ${@arg_FIND_PACKAGE_NAME@_ROOT}")
    else()
      message("poorly_behaved_dependency_dry_run: unknown version, found at ${@arg_FIND_PACKAGE_NAME@_ROOT}")
    endif()
  endif()
else()
  message("poorly_behaved_dependency_dry_run: Not found")
endif()
